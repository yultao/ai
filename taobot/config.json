{
  "preferences": {
    "llmClientType": "openai"
  },
  "apiProviders": {
    "openrouter": {
      "apiKey": "OPENROUTER_API_KEY",
      "apiBaseURL": "https://openrouter.ai/api/v1",
      "enabled": false
    },
    "groq": {
      "apiKey": "GROQ_API_KEY",
      "apiBaseURL": "https://api.groq.com/openai/v1",
      "enabled": true
    }
  },
  "models": {
    "deepseek/deepseek-chat-v3-0324:free": {
      "enabled": false
    },
    "meta-llama/llama-4-scout-17b-16e-instruct": {
      "name": "Meta Llama 4",
      "enabled": true
    }
  },
  "mcpServers": {
    "fetch": {
      "disabled": false,
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "slack-mcp": {
      "disabled": true,
      "command": "node",
      "args": [
        "C:/Workspace/ai/slack-mcp-server/dist/index.js"
      ],
      "env": {
        "CONTEXT_ROOT":"xx"
      }
    },
    "file": {
      "disabled": false,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\Workspace\\ai"
      ]
    },
    "excel": {
      "disabled": false,
      "command": "node",
      "args": [
       "C:/Workspace/ai/excel-mcp-server/dist/index.js"
      ]
    }
  },
  "embedding": {
    "model": "Xenova/distiluse-base-multilingual-cased-v1",
    "embeddingKey": "sk-this-will-be-replaced-by-env",
    "embeddingBaseURL": "https://openrouter.ai/api/v1"
  }
}